# Example environment variables for frontend
# Copy this to .env.local and customize for your local development

# Backend API URL
NEXT_PUBLIC_API_URL=

# Force chunked upload mode for testing (LOCAL DEVELOPMENT ONLY)
# Set to "true" to bypass single-file upload and always use chunked upload
# This allows testing the Azure Block Blob API fallback logic
# WARNING: Only works when NODE_ENV=development
NEXT_PUBLIC_FORCE_CHUNKED_UPLOAD=false

# telemetry
NEXT_PUBLIC_LANGFUSE_PUBLIC_KEY=
NEXT_PUBLIC_LANGFUSE_HOST=
NEXT_PUBLIC_POSTHOG_API_KEY=
NEXT_PUBLIC_POSTHOG_HOST=
NEXT_PUBLIC_SANITY_PROJECT_ID=
NEXT_PUBLIC_SANITY_DATASET=
NEXT_PUBLIC_SANITY_API_VERSION=
